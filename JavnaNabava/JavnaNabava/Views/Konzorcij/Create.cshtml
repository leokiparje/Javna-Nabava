@model Konzorcij

@{
    ViewBag.Title = "Dodavanje konzorcija";
}

<form asp-action="Create" method="post">
    <div asp-validation-summary="All"></div>
    @*<div class="form-group">
        <label asp-for="IdKonzorcij"></label>
        <input asp-for="IdKonzorcij" class="form-control" />
    </div>*@
    <div class="form-group">
        <label asp-for="NazivKonzorcij"></label>
        <input asp-for="NazivKonzorcij" class="form-control" />
    </div>
    <div class="text-center">
        <button class="btn btn-primary" type="submit">Dodaj</button>
        <a asp-action="Index" class="btn btn-secondary">Odustani</a>
    </div>
</form>

@section scripts {
    <partial name="IncludeValidation" />
}